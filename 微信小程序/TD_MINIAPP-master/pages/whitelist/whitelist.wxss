page{
  height:100%;
}

.whitelistpage{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background-color: white;
  overflow-x: hidden;
}

.bg_image{
  width: 100%;
  height: 710rpx;
}


.btn_invite{
  background: #DEC188;
  border-radius: 2px;
  width: 670rpx;
  height: 120rpx;
  font-size: 40rpx;
  display: flex;
  margin-top: 114rpx;
  align-items: center;
  justify-content: center;
}


/******启动页*******/
.startpage{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.start_image{
  width: 168rpx;
  height: 168rpx;
  margin-top: 174rpx;
}

.start_txt{
  color: #353648;
  font-size: 40rpx;
  line-height: 56rpx;
  margin-top: 26rpx;
}

.start_line{
  background-color: #DEDEDE;
  width: 92%;
  height: 1rpx;
  margin-top: 60rpx;
  margin-right: 4%;
  margin-left: 4%;
}

.start_title_view{
  display: flex;
  flex-direction: column;
  margin-top: 64rpx;
  width: 92%;
  margin-left: 4%;
  margin-right: 4%;
}

.start_main_title{
  color: #353648;
  font-size: 36rpx;
  line-height: 50rpx;
}

.start_sub_title{
  color: #757685;
  font-size: 30rpx;
  line-height: 42rpx;
  margin-top: 30rpx;
}

.start_login_btn{
  width: 92%;
  margin-top: 100rpx;
}

.img_luncher{
  width: 750rpx;
  height: 1334rpx;
}



/*小红电*/

.whitelistpage_red{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background: linear-gradient(rgb(53, 43, 47), rgb(45, 32, 38));
  overflow-x: hidden;
}

.bg_image_red{
  margin-top: 200rpx;
  width: 660rpx;
  height: 328rpx;
}

.btn_invite_red{
  background: #ffffff;
  border-radius: 2px;
  width: 670rpx;
  height: 120rpx;
  font-size: 40rpx;
  display: flex;
  margin-top: 320rpx;
  align-items: center;
  justify-content: center;
}