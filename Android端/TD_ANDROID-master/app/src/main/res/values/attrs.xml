<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressViewAttrs">
        <attr name="bgColor" format="color"></attr>
        <attr name="circleWidth" format="dimension"></attr>
        <attr name="progressColor" format="color"></attr>
        <attr name="progress" format="float"></attr>
    </declare-styleable>
    <declare-styleable name="customGridViewAttrs">
        <attr name="itemHeight" format="dimension"></attr>
        <attr name="itemSelectedColor" format="color"></attr>
        <attr name="itemSelectedTextColor" format="color"></attr>
        <attr name="leftSpace" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="TextImageView">
        <attr name="drawableStartWidth" format="dimension"/>
        <attr name="drawableStartHeight" format="dimension"/>
        <attr name="drawableTopWidth" format="dimension"/>
        <attr name="drawableTopHeight" format="dimension"/>
        <attr name="drawableEndWidth" format="dimension"/>
        <attr name="drawableEndHeight" format="dimension"/>
        <attr name="drawableBottomWidth" format="dimension"/>
        <attr name="drawableBottomHeight" format="dimension"/>
    </declare-styleable>
</resources>