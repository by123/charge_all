<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <View
            android:id="@+id/line_1"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:background="@color/item_back"
            app:layout_constraintTop_toTopOf="parent"/>

    <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="2018-02-10"
            android:textColor="@color/tv_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/line_1"/>

    <TextView
            android:id="@+id/tv_refund_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="不可提现"
            android:textColor="@color/tv_black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/tv_time"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_time"/>

    <View
            android:id="@+id/line"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="14dp"
            android:layout_marginRight="16dp"
            android:background="@color/tv_hint"
            app:layout_constraintTop_toBottomOf="@+id/tv_refund_status"/>

    <TextView
            android:id="@+id/tv_income"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="收益金额"
            android:textColor="@color/tv_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/line"/>

    <TextView
            android:id="@+id/tv_income_value1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="0.0元"
            android:textColor="@color/tv_black"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_income"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_income"/>


    <TextView
            android:id="@+id/tv_profit_share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:text="设备分润收入"
            android:textColor="@color/tv_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_income"/>

    <TextView
            android:id="@+id/tv_profit_share_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="10元"
            android:textColor="@color/tv_black"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_profit_share"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_profit_share"/>


    <TextView
            android:id="@+id/tv_refund"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:text="退款分润扣除"
            android:textColor="@color/tv_color"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_profit_share_value"/>

    <TextView
            android:id="@+id/tv_refund_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="0.0元"
            android:textColor="@color/tv_black"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_refund"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_refund"/>


    <TextView
            android:id="@+id/tv_withdrawal_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="15dp"
            android:text="预计收益计提时间"
            android:textColor="@color/tv_color"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_refund"/>

    <TextView
            android:id="@+id/tv_withdrawal_time_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="2018-09-11"
            android:textColor="@color/tv_black"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_withdrawal_time"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_withdrawal_time"/>

</androidx.constraintlayout.widget.ConstraintLayout>
